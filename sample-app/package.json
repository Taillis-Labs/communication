{
  "name": "evaluation",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "scripts": {
    "start": "npx forever server.js",
    "dev": "npx cross-env NODE_ENV=development npm-run-all --parallel dev:client dev:server",
    "dev:server": "npm run dev:server:prebuild && npm-run-all --parallel dev:server:build dev:server:start",
    "dev:server:prebuild": "npx cross-env BABEL_ENV=server npx webpack --config webpack/webpack.server.dev.js",
    "dev:server:build": "npx cross-env BABEL_ENV=server npx webpack --config webpack/webpack.server.dev.js --watch --colors --display-error-details",
    "dev:server:start": "cd build && npx nodemon server.js --watch server.js",
    "dev:client": "npx cross-env BABEL_ENV=browser-dev npx webpack-dev-server --config webpack/webpack.client.dev.js",
    "build": "rm -rf build && npm run build:server && npm run build:client && cp -R config bdd knexfile.js package.json package-lock.json build",
    "build:server": "npx cross-env BABEL_ENV=server npx webpack --config webpack/webpack.server.prod.js --progress --color",
    "build:client": "npx cross-env BABEL_ENV=browser-prod npx webpack --config webpack/webpack.client.prod.js --progress --color",
    "bdd:init": "npx npm-run-all bdd:schema:init bdd:data:init",
    "bdd:production:init": "npx cross-env NODE_ENV=production npx npm-run-all bdd:schema:init bdd:data:init",
    "bdd:schema:init": "npx knex migrate:latest",
    "bdd:production:schema:init": "npx cross-env NODE_ENV=production npx knex migrate:latest",
    "bdd:staging:schema:init": "npx cross-env NODE_ENV=staging npx knex migrate:latest",
    "bdd:schema:rollback": "npx knex migrate:rollback",
    "bdd:data:init": "npx knex seed:run",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
